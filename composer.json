{
    "name": "cheehau/filament-copy-action",
    "description": "This package give actions copyable feature",
    "keywords": [
        "cheehau",
        "laravel",
        "filament-copy-action"
    ],
    "homepage": "https://github.com/cheehau/filament-copy-action",
    "support": {
        "issues": "https://github.com/cheehau/filament-copy-action/issues",
        "source": "https://github.com/cheehau/filament-copy-action"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chee Hau",
            "email": "cheehau@bizportal.cc",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cheehau\\FilamentCopyAction\\": "src/",
            "Cheehau\\FilamentCopyAction\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cheehau\\FilamentCopyAction\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cheehau\\FilamentCopyAction\\FilamentCopyActionServiceProvider"
            ],
            "aliases": {
                "FilamentCopyAction": "Cheehau\\FilamentCopyAction\\Facades\\FilamentCopyAction"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
